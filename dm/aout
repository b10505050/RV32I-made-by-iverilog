T_0 ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x579616c682f0_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x579616c682f0_0;
    %cmpi/s 65536, 0, 32;
    %jmp/0xz T_0.1, 5;
    %pushi/vec4 0, 0, 32;
    %ix/getv/s 4, v0x579616c682f0_0;
    %store/vec4a v0x579616c68420, 4, 0;
    %load/vec4 v0x579616c682f0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x579616c682f0_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %end;
    .thread T_0;
    .scope S_0x579616c4e9c0;
T_1 ;
    %wait E_0x579616c4b590;
    %load/vec4 v0x579616c680b0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %load/vec4 v0x579616c685c0_0;
    %load/vec4 v0x579616c68170_0;
    %parti/s 16, 0, 2;
    %ix/load 4, 2, 0;
    %flag_set/imm 4, 0;
    %shiftr 4;
    %pad/u 18;
    %ix/vec4 3;
    %ix/load 4, 0, 0; Constant delay
    %assign/vec4/a/d v0x579616c68420, 0, 4;
T_1.0 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x579616c4e9c0;
T_2 ;
    %wait E_0x579616c4b0c0;
    %load/vec4 v0x579616c4ec80_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_2.0, 8;
    %load/vec4 v0x579616c68170_0;
    %parti/s 16, 0, 2;
    %ix/load 5, 2, 0;
    %flag_set/imm 4, 0;
    %shiftr 5;
    %pad/u 18;
    %ix/vec4 4;
    %load/vec4a v0x579616c68420, 4;
    %assign/vec4 v0x579616c684e0_0, 0;
T_2.0 ;
    %jmp T_2;
    .thread T_2;
    .scope S_0x579616c3cde0;
T_3 ;
    %delay 50, 0;
    %load/vec4 v0x579616c689f0_0;
    %inv;
    %store/vec4 v0x579616c689f0_0, 0, 1;
    %jmp T_3;
