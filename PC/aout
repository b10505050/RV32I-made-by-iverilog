#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 10;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x651414e0f6f0 .scope module, "PC_tb" "PC_tb" 2 3;
 .timescale -9 -10;
v0x651414e21c40_0 .var "clk", 0 0;
v0x651414e21d00_0 .var "next_pc", 31 0;
v0x651414e21dd0_0 .net "pc", 31 0, v0x651414e219e0_0;  1 drivers
v0x651414e21ed0_0 .var "reset", 0 0;
S_0x651414e0f880 .scope module, "uut" "PC" 2 13, 3 3 0, S_0x651414e0f6f0;
 .timescale -9 -10;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "reset";
    .port_info 2 /INPUT 32 "next_pc";
    .port_info 3 /OUTPUT 32 "pc";
v0x651414dd5ba0_0 .net "clk", 0 0, v0x651414e21c40_0;  1 drivers
v0x651414e21900_0 .net "next_pc", 31 0, v0x651414e21d00_0;  1 drivers
v0x651414e219e0_0 .var "pc", 31 0;
v0x651414e21ad0_0 .net "reset", 0 0, v0x651414e21ed0_0;  1 drivers
E_0x651414e10810 .event posedge, v0x651414e21ad0_0, v0x651414dd5ba0_0;
    .scope S_0x651414e0f880;
T_0 ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x651414e219e0_0, 0, 32;
    %end;
    .thread T_0;
    .scope S_0x651414e0f880;
T_1 ;
    %wait E_0x651414e10810;
    %load/vec4 v0x651414e21ad0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0x651414e219e0_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x651414e21900_0;
    %assign/vec4 v0x651414e219e0_0, 0;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x651414e0f6f0;
T_2 ;
    %delay 50, 0;
    %load/vec4 v0x651414e21c40_0;
    %inv;
    %store/vec4 v0x651414e21c40_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x651414e0f6f0;
T_3 ;
    %vpi_call 2 26 "$dumpfile", "PC_tb.vcd" {0 0 0};
    %vpi_call 2 27 "$dumpvars" {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x651414e21c40_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x651414e21ed0_0, 0, 1;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x651414e21d00_0, 0, 32;
    %delay 100, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x651414e21ed0_0, 0, 1;
    %delay 100, 0;
    %pushi/vec4 4, 0, 32;
    %store/vec4 v0x651414e21d00_0, 0, 32;
    %delay 100, 0;
    %pushi/vec4 8, 0, 32;
    %store/vec4 v0x651414e21d00_0, 0, 32;
    %delay 100, 0;
    %pushi/vec4 12, 0, 32;
    %store/vec4 v0x651414e21d00_0, 0, 32;
    %delay 100, 0;
    %pushi/vec4 16, 0, 32;
    %store/vec4 v0x651414e21d00_0, 0, 32;
    %delay 100, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x651414e21ed0_0, 0, 1;
    %delay 100, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x651414e21ed0_0, 0, 1;
    %delay 200, 0;
    %vpi_call 2 45 "$finish" {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
